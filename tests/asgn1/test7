begin
	if (3<4) 
		printInt(3+4);
	if (3<=4) 
		printInt(3-4);
	if (3!=4) 
		printInt(3*4);
	if (3==4) 
		printInt(3/4);
	if (3>4) 
		printInt(3%4);
	while (3>=4) 
		printInt(3<<4);
	printReal(3*PI);
	printStr("Apple pie");
end;
